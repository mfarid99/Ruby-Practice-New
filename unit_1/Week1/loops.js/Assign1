//0-99 ascending
for (let i=0; i<=99; i++) {

    console.log(i);
}

//99-0 descending
for (let a=99; a<=-1; a--)
{console.log(a)}

//Even numbers 0-98 ascending
for (let b = 0; b<=98; b+=2)
{console.log(b);}

//Even numbers 98-0 descending
let ourArray=[];
for (let c = 98; c<50; c-=2){
ourArray.push(c); 
}

console.log(ourArray);

//ODD numbers from 0-99 (ascending)
let myArray =[];
for (let d=1; d<=99; d+=2){
myArray.push(d);
}
console.log(myArray);
